@model Project
@{
    ViewData["Title"] = "ProjectDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table>
    <thead>
        <tr>
            <td>عکس</td>
            <td>نام</td>
            <td>توضیحات</td>
            <td>مشتری</td>
        </tr>
    </thead>
    <tbody>
    <td><img src="~/assets/images/project/@Model.Image"/></td>
    <td>@Model.Name</td>
    <td>@Model.Description</td>
    <td>@Model.Client</td>
    </tbody>
</table>

